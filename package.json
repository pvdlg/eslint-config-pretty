{
  "name": "@metahub/eslint-config",
  "description": "A set of shareable eslint configs",
  "version": "1.5.0",
  "author": "Pierre Vanduynslager",
  "bugs": {
    "url": "https://github.com/vanduynslagerp/eslint-config/issues"
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.2.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.7.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "prettier": "^1.5.2"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "codecov": "^2.2.0",
    "eslint-find-rules": "^3.1.1",
    "lodash": "^4.17.4",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1"
  },
  "homepage": "https://github.com/vanduynslagerp/eslint-config#readme",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "babel-eslint": ">=7.2.3",
    "eslint": ">=4.2.0",
    "eslint-config-prettier": ">=2.3.0",
    "eslint-plugin-ava": ">=4.2.1",
    "eslint-plugin-babel": ">=4.1.1",
    "eslint-plugin-eslint-comments": ">=1.0.2",
    "eslint-plugin-import": ">=2.7.0",
    "eslint-plugin-jasmine": ">=2.7.1",
    "eslint-plugin-json": ">=1.2.0",
    "eslint-plugin-node": ">=5.1.0",
    "eslint-plugin-prettier": ">=2.1.2",
    "eslint-plugin-promise": ">=3.5.0",
    "eslint-plugin-sort-class-members": ">=1.1.1",
    "prettier": ">=1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanduynslagerp/eslint-config.git"
  },
  "scripts": {
    "clean": "rimraf coverage && rimraf .nyc_output",
    "lint": "eslint *-config.js index.js package.json test",
    "pretest": "npm run lint && npm run clean",
    "test": "nyc --all ava -v && nyc report --reporter=lcov",
    "codecov": "codecov",
    "prepublishOnly": "npm run test"
  },
  "nyc": {
    "include": [
      "{*-config,index}.js"
    ]
  }
}
