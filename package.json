{
  "name": "@metahub/eslint-config",
  "description": "A set of shareable eslint configs",
  "version": "1.1.3",
  "author": "Pierre Vanduynslager",
  "bugs": {
    "url": "https://github.com/vanduynslagerp/eslint-config/issues"
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.1.1",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jasmine": "^2.6.2",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-promise": "^3.5.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "codecov": "^2.2.0",
    "del-cli": "^1.1.0",
    "eslint-find-rules": "^3.1.1",
    "lodash": "^4.17.4",
    "nyc": "^11.0.3"
  },
  "homepage": "https://github.com/vanduynslagerp/eslint-config#readme",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "babel-eslint": ">=7.2.3",
    "eslint": ">=4.0.0",
    "eslint-plugin-ava": ">=4.2.0",
    "eslint-plugin-babel": ">=4.1.1",
    "eslint-plugin-import": ">=2.3.0",
    "eslint-plugin-jasmine": ">=2.3.2",
    "eslint-plugin-json": ">=1.2.0",
    "eslint-plugin-promise": ">=3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanduynslagerp/eslint-config.git"
  },
  "scripts": {
    "clean": "del coverage && del .nyc_output",
    "lint": "eslint *-config.js index.js package.json",
    "pretest": "npm run lint && npm run clean",
    "test": "nyc --all ava -v && nyc report --reporter=lcov",
    "codecov": "codecov"
  },
  "nyc": {
    "include": [
      "{*-config,index}.js"
    ]
  }
}
